<project name="broker" default="run" basedir="."> 
    <import>
    <javaresource name="demo.xml">
        <classpath/>
    </javaresource>
    </import>

    <import>
    <javaresource name="syntax-tree.xml">
        <classpath/>
    </javaresource>
    </import>

    <import>
    <javaresource name="normalised-syntax-tree.xml">
        <classpath/>
    </javaresource>
    </import>

    <import>
    <javaresource name="aliases.xml">
        <classpath/>
    </javaresource>
    </import>

    <import>
    <javaresource name="symbol-table.xml">
        <classpath/>
    </javaresource>
    </import>

    <import>
    <javaresource name="call-graph.xml">
        <classpath/>
    </javaresource>
    </import>

    <import>
    <javaresource name="call-sites.xml">
        <classpath/>
    </javaresource>
    </import>

    <import>
    <javaresource name="control-flow.xml">
        <classpath/>
    </javaresource>
    </import>

    <import>
    <javaresource name="visualisation.xml">
        <classpath/>
    </javaresource>
    </import>

    <extension-point name="demo" depends=""/>
    <extension-point name="syntax-tree" depends="demo"/>

    <extension-point name="normalised-syntax-tree" depends="syntax-tree"/>

    <extension-point name="aliases" depends="syntax-tree"/>
    <extension-point name="symbol-table" depends="aliases"/>

    <extension-point name="call-graph" depends="aliases, symbol-table"/>

    <extension-point name="call-sites" depends="symbol-table, aliases, syntax-tree"/>
    <extension-point name="control-flow" depends="symbol-table, aliases, syntax-tree"/>

    <extension-point name="visualisation" depends="syntax-tree, aliases, symbol-table, call-graph, control-flow, call-sites"/>


    <target name="run" depends="visualisation">  
    </target>
</project>
