<project name="broker" default="run" basedir="."> 
    <import>
    <javaresource name="xyz.xml">
        <classpath/>
    </javaresource>
    </import>

    <import>
    <javaresource name="everything-until-now.xml">
        <classpath/>
    </javaresource>
    </import>

    <import>
    <javaresource name="syntax-tree.xml">
        <classpath/>
    </javaresource>
    </import>

    <import>
    <javaresource name="symbol-table.xml">
        <classpath/>
    </javaresource>
    </import>

    <import>
    <javaresource name="call-graph.xml">
        <classpath/>
    </javaresource>
    </import>

    <import>
    <javaresource name="visualisation.xml">
        <classpath/>
    </javaresource>
    </import>

    <extension-point name="xyz" depends=""/>
    <extension-point name="syntax-tree" depends="xyz"/>
    <extension-point name="everything-until-now" depends="syntax-tree"/>
    <extension-point name="symbol-table" depends="everything-until-now"/>
    <extension-point name="call-graph" depends="everything-until-now, symbol-table"/>
    <extension-point name="visualisation" depends="syntax-tree, everything-until-now, symbol-table, call-graph"/>

    <target name="run" depends="syntax-tree, everything-until-now, symbol-table, call-graph, visualisation">  
    </target>
</project>
