<project name="broker" default="run" basedir="."> 
    <import>
    <javaresource name="demo.xml">
        <classpath/>
    </javaresource>
    </import>

    <import>
    <javaresource name="everything-until-now.xml">
        <classpath/>
    </javaresource>
    </import>

    <import>
    <javaresource name="syntax-tree.xml">
        <classpath/>
    </javaresource>
    </import>

    <import>
    <javaresource name="aliases.xml">
        <classpath/>
    </javaresource>
    </import>

    <import>
    <javaresource name="symbol-table.xml">
        <classpath/>
    </javaresource>
    </import>

    <import>
    <javaresource name="call-graph.xml">
        <classpath/>
    </javaresource>
    </import>

    <import>
    <javaresource name="visualisation.xml">
        <classpath/>
    </javaresource>
    </import>

    <extension-point name="demo" depends=""/>
    <extension-point name="syntax-tree" depends="demo"/>
    <extension-point name="aliases" depends="syntax-tree"/>
    <extension-point name="symbol-table" depends="aliases"/>
    <extension-point name="call-graph" depends="aliases, symbol-table"/>
    <extension-point name="everything-until-now" depends="symbol-table, aliases, syntax-tree"/>
    <extension-point name="visualisation" depends="syntax-tree, aliases, symbol-table, call-graph"/>


    <target name="run" depends="visualisation">  
    </target>
</project>
