<project name="p4parser" default="test" basedir="."> 
    <target name="p4parser" extensionOf="syntax-tree">
        <java classname="p4analyser.experts.syntaxtree.AntlrP4">
            <arg value="${host}"/>
            <arg value="${port}"/>
